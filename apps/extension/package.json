{
  "name": "@henry/extension",
  "displayName": "Henry Extension",
  "version": "0.0.1",
  "description": "extension",
  "author": "Leo Wang",
  "scripts": {
    "clean": "git clean -xdf .plasmo build node_modules",
    "dev": "plasmo dev --env=../../.env",
    "build": "plasmo build --env=../../.env",
    "package": "plasmo package --env=../../.env",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@henry/api": "workspace:*",
    "@henry/db": "workspace:*",
    "@henry/ui": "workspace:*",
    "@henry/validators": "workspace:*",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-query": "^5.35.5",
    "@tanstack/react-router": "^1.34.3",
    "@trpc/client": "11.0.0-rc.364",
    "@trpc/react-query": "11.0.0-rc.364",
    "geist": "^1.3.0",
    "lucide-react": "^0.379.0",
    "next": "^14.2.3",
    "plasmo": "0.86.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superjson": "2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@henry/eslint-config": "workspace:*",
    "@henry/prettier-config": "workspace:*",
    "@henry/tailwind-config": "workspace:*",
    "@henry/tsconfig": "workspace:*",
    "@types/chrome": "0.0.258",
    "@types/node": "^20.12.9",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  },
  "prettier": "@henry/prettier-config"
}
